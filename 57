DEFAULT SYSDATETIME() FOR FechaFinal;





ALTER TABLE BANDEJA_ENVIO_SMS
ALTER COLUMN FechaInicio DATETIME2 NOT NULL;

ALTER TABLE BANDEJA_ENVIO_SMS
